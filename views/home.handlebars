<h1>Cadastrar cliente</h1>
<form action="/cliente/insert" method="post" id="cadForm">
    <div class="form-control">
        <label for="nome">Nome</label>
        <input type="text" name="nome" maxlength="70" required>
        
        <label for="sexo">Sexo</label>
        <select name="sexo">
            <option value="M">Masculino</option>
            <option value="F">Feminino</option>
        </select>
        
        <label for="nascimento">Data de Nascimento</label>
        <input type="date" name="nascimento" required>
        
        <label for="cep">CEP</label>
        <input type="text" name="cep" maxlength="8" required>
        
        <label for="telefone">Telefone</label>
        <input type="tel" name="telefone" maxlength="11" required>
        
        <label for="email">Email</label>
        <input type="text" name="email" required>

    </div>
    <input type="submit" value="Cadastrar" id="submit">
</form>